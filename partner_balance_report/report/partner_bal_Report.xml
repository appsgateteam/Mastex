<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <template id="partner_balance_report_view">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
            <!-- <t t-call="web.html_container"> -->
                <div class="page" style="font-size:13px;">
                            
                    <h3 class="text-center">Partner Balance</h3>

                    <table class="table noborder mb0" style="border:0;width:100%;height:100px;padding:0;">
                      <tr style="padding:0;">
                        <td class="text-left" style="border:0;width:20%;padding:0;">
                        <span>Start Date</span>
                        </td>
                        <td class="text-left" style="border:0;width:30%;padding:0;">
                          <span t-esc="start_date"/>
                        </td>
                        <td class="text-left" style="border:0;width:20%;padding:0;">
                        <span>End Date</span>
                        </td>
                        <td class="text-left" style="border:0;background-color:transparent;width:30%;padding:0;">
                            <span t-esc="end_date"/>
                        </td>
                      </tr>
                      
                    </table>
                                
                    <div>
                        <table class="table table-condensed table-bordered" style="font-size:13px;">
                            
                            <thead>
                              <tr>
                                <th class="text-center" colspan="1" style="width: 10%;color:black;" ><span>Partner Name</span>  </th>
                                <th class="text-center" colspan="4" style="width: 30%;color:black;" ><span>Initial balanace</span>  </th>
                                <th class="text-center" colspan="4" style="width: 30%;color:black;" > <span>Transaction</span> </th>
                                <th class="text-center" colspan="4" style="width: 30%;color:black;" ><span>Ending Balance</span>  </th>
                                
                              </tr>
                              <tr>
                                <th class="text-center"  style="width: 10%;color:black;" ></th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Debit</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" > <span>Credit</span> </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Balance</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Foreign Balance</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Debit</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Credit</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Balance</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Foreign Balance</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Debit</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Credit</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Balance</span>  </th>
                                <th class="text-center"  style="width: 7.5%;color:black;" ><span>Foreign Balance</span>  </th>
                                
                              </tr>
                                
                            </thead>
                            <tbody>
                                <t t-foreach="docs" t-as="doc">
                                  <tr>
                                    <th class="text-center"  style="width: 10%;color:black;" ><span t-esc="doc['name']" /> </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['init_dr']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/> </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" > <span t-esc="doc['init_cr']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>  </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['init_bal']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['init_for']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['trn_dr']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>  </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['trn_cr']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['trn_bal']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>  </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['trn_for']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['Ending_dr']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['Ending_cr']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['Ending_bal']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    <th class="text-center"  style="width: 7.5%;color:black;" ><span t-esc="doc['ending_for']" t-options="{&quot;widget&quot;:&quot;float&quot;,&quot;precision&quot;:2}"/>   </th>
                                    
                                </tr>
                                </t>
                                
                            </tbody>
                        </table>
                    </div>
            
                </div>
            </t>
            </t>
    </template>

    <!-- <template id="job_costing_report_view">
        <t t-call="web.html_container">
            
                <t t-call="marcoms_updates.job_costing_report_view_document" />
            
        </t>
    </template> -->
</odoo>