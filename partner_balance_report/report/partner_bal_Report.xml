<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <template id="partner_balance_report_view">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
            <!-- <t t-call="web.html_container"> -->
                <div class="page" style="font-size:13px;">
                            
                    <h3 class="text-center">Partner Balance</h3>

                    <table class="table noborder mb0" style="border:0;width:100%;height:100px;padding:0;">
                      <tr style="padding:0;">
                        <td class="text-left" style="border:0;width:20%;padding:0;">
                        <span>Start Date</span>
                        </td>
                        <td class="text-left" style="border:0;width:30%;padding:0;">
                          <span t-esc="start_date"/>
                        </td>
                        <td class="text-left" style="border:0;width:20%;padding:0;">
                        <span>End Date</span>
                        </td>
                        <td class="text-left" style="border:0;background-color:transparent;width:30%;padding:0;">
                            <span t-esc="end_date"/>
                        </td>
                      </tr>
                      
                    </table>
                                
                    <div>
                        <table class="table table-condensed table-bordered" style="font-size:13px;">
                            
                            <thead>
                              <tr>
                                <th class="text-center" colspan="4" style="width: 33.3%;color:black;" ><span>Initial balanace</span>  </th>
                                <th class="text-center" colspan="4" style="width: 33.3%;color:black;" > <span>transaction</span> </th>
                                <th class="text-center" colspan="4" style="width: 33.3%;color:black;" ><span>ending balance</span>  </th>
                                
                              </tr>
                              <tr>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>debit</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" > <span>credit</span> </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>balance</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>foreign balance</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>debit</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>credit</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>balance</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>foreign balance</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>debit</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>credit</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>balance</span>  </th>
                                <th class="text-center"  style="width: 8.3%;color:black;" ><span>foreign balance</span>  </th>
                                
                              </tr>
                                
                            </thead>
                            <tbody>
                                <t t-foreach="docs" t-as="doc">
                                  <tr>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_dr']" /> </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" > <span t-esc="doc['init_cr']" />  </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_bal']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_bal']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['trn_dr']" />  </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['trn_cr']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['trn_bal']" />  </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['trn_bal']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_dr']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_cr']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_bal']" />   </th>
                                    <th class="text-center"  style="width: 8.3%;color:black;" ><span t-esc="doc['init_bal']" />   </th>
                                    
                                </tr>
                                </t>
                                
                            </tbody>
                        </table>
                    </div>
            
                </div>
            </t>
            </t>
    </template>

